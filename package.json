{
  "name": "ts2gas",
  "version": "4.2.0",
  "description": "A function that transpiles TypeScript to Google Apps Script.",
  "type": "module",
  "files": [
    "src"
  ],
  "exports": "./src/index.js",
  "main": "src/ts2gas.cjs",
  "types": "src/index.d.ts",
  "engines": {
    "node": "^12.20.0 || ^14.13.1 || >=16.0.0"
  },
  "license": "MIT",
  "author": "Grant Timmerman",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/grant/ts2gas.git"
  },
  "bugs": {
    "url": "https://github.com/grant/ts2gas/issues"
  },
  "homepage": "https://github.com/grant/ts2gas#readme",
  "scripts": {
    "build": "tsc --project tsconfig.json",
    "dryrun": "npm publish --dry-run",
    "lint": "xo && echo 'No lint errors. All good!'",
    "test": "npm run lint && node tests/test.js"
  },
  "dependencies": {
    "type-fest": "^2.1.0",
    "typescript": "^4.4.2"
  },
  "devDependencies": {
    "prettier": "^2.3.2",
    "xo": "^0.44.0"
  },
  "xo": {
    "ignores": [
      "**.js"
    ],
    "space": 2,
    "rules": {
      "unicorn/prefer-node-protocol": "warn"
    },
    "prettier": true
  }
}
